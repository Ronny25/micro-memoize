{
  "env": {
    "development": {
      "plugins": [
        "@babel/plugin-syntax-flow",
        "@babel/plugin-transform-flow-strip-types"
      ],
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ]
    },
    "es": {
      "plugins": [
        "@babel/plugin-syntax-flow",
        "@babel/plugin-transform-flow-strip-types"
      ],
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ]
    },
    "lib": {
      "plugins": [
        "@babel/plugin-syntax-flow",
        "@babel/plugin-transform-flow-strip-types"
      ],
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        "@babel/plugin-syntax-flow",
        "@babel/plugin-transform-flow-strip-types"
      ],
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        "@babel/plugin-syntax-flow",
        "@babel/plugin-transform-flow-strip-types",
        "@babel/plugin-transform-runtime"
      ],
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true
          }
        ]
      ]
    }
  }
}
